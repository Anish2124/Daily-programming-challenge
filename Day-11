-----------------------Permutations of a String----------------------

import java.util.*;

public class PermuteSimple {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        // taking input from user
        String s = sc.nextLine();

        Set<String> ans = new HashSet<>();
        findPermutations(s, "", ans);
        for (String p : ans) {
            System.out.println(p);
        }

        sc.close();
    }

    // function to generate permutations
    public static void findPermutations(String str, String current, Set<String> result) {
        if (str.length() == 0) {
            result.add(current);
            return;
        }

        for (int i = 0; i < str.length(); i++) {
            String left = str.substring(0, i);
            String right = str.substring(i + 1);
            String remaining = left + right;

            // recursive call
            findPermutations(remaining, current + str.charAt(i), result);
        }
    }
}
